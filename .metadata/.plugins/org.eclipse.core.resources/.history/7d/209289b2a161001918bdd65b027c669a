package day1303;

import java.io.FileInputStream;

public class Test2 {
	public static void main(String[] args) throws Exception {
		FileInputStream fis = new FileInputStream("d:/abc/f2");
		// 标准循环读取格式
		int b;
		while ((b = fis.read()) != -1) {

		}
		while (true) {
			int s = fis.read();
			if (s == -1) {
				break;
			}
			System.out.println(s);
		}
	}
}
