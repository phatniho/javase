package day1303;

import java.io.FileInputStream;
import java.io.FileNotFoundException;

public class Test2 {
	public static void main(String[] args) throws Exception {
		FileInputStream fis = new FileInputStream("d:/abc/f2");
		while (true) {
			int s = fis.read();
			if (s == -1) {
				break;
			}
			System.out.println(s);
		}
	}
}
